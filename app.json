{
  "expo": {
    "name": "BirthdayCountdown",
    "slug": "BirthdayCountdown",
    "description": "This App shows you how long it is until Sergei's next birthday",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/birthday.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
